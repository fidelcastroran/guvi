
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
 
  let a = parseInt(userInput[0])
    
  for(i=0;i<2*a;i++){
       
      let b = i>a ? 2*a-i : i;
        let str =''
      for(j=0;j<b;j++){
          str += '*'
            
      }    
      console.log(str)
  }
})